@model List<User>;


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://fonts.googleapis.com/css2?family=Lobster&family=Russo+One&family=Send+Flowers&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Outfit:wght@500;700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css" integrity="sha512-z3gLpd7yknf1YoNbCzqRKc4qyor8gaKU1qmn+CShxbuBusANI9QpRohGBreCFkKxLhei6S9CQXFEbbKuqLg0DA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="stylesheet" href="/css/YeniUser.css">
    <string src="https://code.jquery.com/jquery-3.7.1.min.js"></string>
    <title>User</title>
</head>
<body>

    <section class="iki">
        <div class="uc">
            <img src="/img//gg2.jpeg" alt="">
            <h1>Fərəcova Esmira</h1>
            <p class="e1">esmirafrcova@gmail.com</p>
            <div class="umumi50">
                <a href="/Home/Index">
                    <div class="umumi51">
                        <h2>Ana Səhifə</h2>
                    </div>
                </a>
                <a href="/Admin/AdminPanel">
                    <div class="umumi51">
                        <h2>Məhsullar</h2>
                    </div>
                </a>
                <a href="">
                    <div class="umumi51">
                        <h2>Kateqoriyalar</h2>
                    </div>
                </a>
                <a href="">
                    <div class="umumi51">
                        <h2>Yeni slayd</h2>
                    </div>
                </a>
                <a href="">
                    <div class="umumi51">
                        <h2>User</h2>
                    </div>
                </a>
                <div class="cixis">
                    <i class="fa-solid fa-arrow-right-from-bracket"></i><p class="e2">Çıxış</p>
                </div>
            </div>
        </div>
        <div class="dord">
            <div class="yeni">
                <a href="/Admin/YeniMehsul">+ Əlavə et</a>
            </div>
            <div class="input">
                <i class="fa-solid fa-magnifying-glass"></i>
                <input type="text" name="" id="" placeholder="">
            </div>
            <div class="scroll-table">
                @{
                    int? sira = 1;
                }
                <table>
                    <thead>
                        <tr>
                            <th>Sıra</th>
                            <th>İstifadəçi Adı</th>
                            <th>Telefon Nömrəsi</th>
                            <th>İstifadəçi Email</th>
                            <th>İstifadəçi Parolu</th>
                            <th>Nömrə Başlığı</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>@(sira++)</td>
                                <td>@item.UserName</td>
                                <td>@item.UserPhone</td>
                                <td>@item.UserEmail</td>
                                <td>@item.UserPassword</td>
                                <td>@item.UserTelefon.TelefonHead</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </section>


    <script src="/js/AdminPanel.js"></script>
</body>
</html>
